<Page x:Class="RundenAnsicht.InitativeChange"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RundenAnsicht"
      mc:Ignorable="d" Name="IniChange" DataContext="{Binding viewModel ,ElementName=IniChange}"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="InitativeChange">

    <Grid>
        <Grid Grid.Row="4" Grid.ColumnSpan="2">

        
            <StackPanel Grid.Column="1">

                <Label Content="Int wert ändern" HorizontalAlignment="Center"/>
                <StackPanel Margin="5 0" Orientation="Horizontal">
                    <StackPanel Margin="0 5" >
                        <TextBlock Margin="5" Text="Teilnehmer"/>
                        <TextBlock  Margin="5" Text="Initativwert"/>
                    </StackPanel>
                    <StackPanel >
                        <TextBox  Name="targetname"  Margin="5" Width="100"/>
                        <TextBox  Name="targetIni"  Margin="5" Width="50"/>
                    </StackPanel>
                </StackPanel>
                <Button x:Name="IntChange" Content="Ändern" Click="IntChange_Click"/>
            </StackPanel>
        </Grid>
    </Grid>
</Page>
